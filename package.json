{
  "name": "marked-react",
  "description": "Render Markdown as React components",
  "version": "1.3.0",
  "type": "module",
  "author": "sibiraj-s",
  "license": "MIT",
  "repository": "https://github.com/sibiraj-s/marked-react.git",
  "bugs": "https://github.com/sibiraj-s/marked-react/issues",
  "homepage": "https://github.com/sibiraj-s/marked-react#readme",
  "main": "./dist/marked-react.cjs",
  "module": "./dist/marked-react.js",
  "exports": {
    ".": {
      "require": "./dist/marked-react.cjs",
      "import": "./dist/marked-react.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "keywords": [
    "react",
    "markdown",
    "marked",
    "markedjs",
    "no dangerouslySetInnerHTML"
  ],
  "files": [
    "src/index.d.ts",
    "dist/"
  ],
  "scripts": {
    "lint": "eslint . --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx",
    "build": "rollup -c",
    "storybook": "start-storybook -p 7007",
    "build:storybook": "build-storybook",
    "test": "jest",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "is-ci || npm run build"
  },
  "peerDependencies": {
    "react": "^16.8.0 || >=17.0.0"
  },
  "dependencies": {
    "@types/marked": "^4.0.7",
    "marked": "^4.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@jest/globals": "^29.2.2",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "commitlint-config-non-conventional": "^1.0.1",
    "eslint": "^8.26.0",
    "eslint-config-pegasus": "^3.4.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.6",
    "highlight.js": "^11.6.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^29.2.2",
    "lint-staged": "13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-lowlight": "^3.0.0",
    "rollup": "^3.2.3",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}

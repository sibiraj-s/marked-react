{
  "name": "marked-react",
  "description": "Render Markdown as React components",
  "version": "1.3.2",
  "type": "module",
  "author": "sibiraj-s",
  "license": "MIT",
  "repository": "https://github.com/sibiraj-s/marked-react.git",
  "bugs": "https://github.com/sibiraj-s/marked-react/issues",
  "homepage": "https://github.com/sibiraj-s/marked-react#readme",
  "main": "./dist/marked-react.cjs",
  "module": "./dist/marked-react.js",
  "exports": {
    ".": {
      "require": "./dist/marked-react.cjs",
      "import": "./dist/marked-react.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "keywords": [
    "react",
    "markdown",
    "marked",
    "markedjs",
    "no dangerouslySetInnerHTML"
  ],
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "eslint . --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx",
    "build": "rollup -c",
    "dev": "storybook dev -p 7007",
    "build:storybook": "storybook build",
    "test": "jest",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "is-ci || npm run build"
  },
  "peerDependencies": {
    "react": "^16.8.0 || >=17.0.0"
  },
  "dependencies": {
    "@types/marked": "^4.0.7",
    "marked": "^4.2.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@jest/globals": "^29.5.0",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/manager-api": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/react-vite": "^7.0.7",
    "@storybook/theming": "^7.0.7",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "commitlint-config-non-conventional": "^1.0.1",
    "eslint": "^8.39.0",
    "eslint-config-pegasus": "^3.5.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "highlight.js": "^11.7.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.5.0",
    "lint-staged": "13.2.1",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-lowlight": "^3.0.0",
    "rollup": "^3.21.0",
    "storybook": "^7.0.7",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
